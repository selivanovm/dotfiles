URxvt*color8  : #000000
! red
URxvt*color1  : #E52222
URxvt*color9  : #E52222
! green
URxvt*color2  : #A6E32D
URxvt*color10 : #A6E32D
! yellow
URxvt*color3  : #FC951E
URxvt*color11 : #FC951E
! blue
URxvt*color4  : #BF8DFF
URxvt*color12 : #BF8DFF
! magenta
URxvt*color5  : #FA250F
URxvt*color13 : #FA250F
! cyan
URxvt*color6  : #67D9F0
URxvt*color14 : #67D9F0
! white
URxvt*color7  : #F2F2F2
